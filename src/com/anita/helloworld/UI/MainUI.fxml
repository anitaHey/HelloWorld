<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="567.0" prefWidth="604.0" styleClass="main"
            stylesheets="@../css/MainStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.anita.helloworld.Main">
   <children>
      <StackPane layoutX="49.0" prefHeight="567.0" prefWidth="557.0"
                 styleClass="main_stack">
      <children>
         <fx:include fx:id="pane_hello" source="HelloMain.fxml" />
         <fx:include fx:id="pane_list" source="UserListMain.fxml" />
         <fx:include fx:id="pane_chat" source="ChatListMain.fxml" />
         <fx:include fx:id="pane_game" source="GameMain.fxml" />
         <fx:include fx:id="pane_setting" source="SettingMain.fxml" />
			</children>
		</StackPane>
		<VBox alignment="CENTER" prefHeight="567.0" prefWidth="21.0"
         styleClass="main_menu">
         <children>
            <Button fx:id="menu_button_hello" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="65.0">
                     <children>
                        <ImageView fitHeight="34.0" fitWidth="31.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/hi-face-speech-bubble.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
            <Button fx:id="menu_button_list" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="65.0">
                     <children>
                        <ImageView fitHeight="34.0" fitWidth="31.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/list-with-dots.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
            <Button fx:id="menu_button_chat" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="46.0">
                     <children>
                        <ImageView fitHeight="32.0" fitWidth="34.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/chat.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
            <Button fx:id="menu_button_game" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="65.0">
                     <children>
                        <ImageView fitHeight="37.0" fitWidth="32.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/console.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
            <Button fx:id="menu_button_setting" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="65.0">
                     <children>
                        <ImageView fitHeight="32.0" fitWidth="33.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/settings-work-tool.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
            <Button fx:id="menu_button_logout" mnemonicParsing="false"
                    styleClass="main_menu_button">
               <graphic>
                  <HBox alignment="CENTER" prefHeight="65.0">
                     <children>
                        <ImageView fitHeight="39.0" fitWidth="33.0"
                                   pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../image/logout.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
               </graphic>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
