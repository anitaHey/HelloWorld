<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="200.0"
      prefWidth="200.0"
      stylesheets="@../css/ChatRoomStyle.css"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ScrollPane fitToWidth="true"
                    layoutX="1.0"
                    layoutY="49.0"
                    prefHeight="429.0"
                    prefWidth="557.0"
                    styleClass="scrollpane">
            <content>
                <VBox prefWidth="552.0"
                      spacing="17.0"
                      fx:id="chat">
                    <children>
                    </children>
                    <padding>
                        <Insets bottom="15.0"
                                left="15.0"
                                right="15.0"
                                top="15.0" />
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
        <Pane layoutX="1.0"
              layoutY="479.0"
              prefHeight="88.0"
              prefWidth="557.0"
              styleClass="down_pane">
            <children>
                <TextArea layoutX="8.0"
                          layoutY="44.0"
                          maxHeight="41.0"
                          maxWidth="475.0"
                          minHeight="39.0"
                          minWidth="414.0"
                          prefColumnCount="1"
                          prefHeight="39.0"
                          prefWidth="475.0"
                          styleClass="chat_text"
                          wrapText="true"
                          fx:id="text">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </TextArea>
                <Button layoutX="486.0"
                        layoutY="43.0"
                        mnemonicParsing="false"
                        prefHeight="41.0"
                        prefWidth="64.0"
                        styleClass="chat_button"
                        fx:id="send">
                    <graphic>
                        <ImageView fitHeight="30.0"
                                   fitWidth="47.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/right-arrow.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <HBox layoutX="2.0"
                      prefHeight="44.0"
                      prefWidth="495.0">
                    <children>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="37.0"
                                           fitWidth="30.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/photo-camera.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="38.0"
                                           fitWidth="30.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/frame-landscape.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="35.0"
                                           fitWidth="30.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/paper-clip.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="35.0"
                                           fitWidth="30.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/video-camera.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="33.0"
                                           fitWidth="29.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/smiling-emoticon-square-face.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="42.0"
                                prefWidth="70.7"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="34.0"
                                           fitWidth="31.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/call-answer-3.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button mnemonicParsing="false"
                                prefHeight="40.0"
                                prefWidth="64.0"
                                styleClass="chat_button">
                            <graphic>
                                <ImageView fitHeight="34.0"
                                           fitWidth="31.0"
                                           pickOnBounds="true"
                                           preserveRatio="true">
                                    <image>
                                        <Image url="@../image/list.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="4.0" />
                    </padding>
                </HBox>
            </children>
        </Pane>
        <HBox alignment="CENTER_LEFT"
              layoutX="1.0"
              prefHeight="51.0"
              prefWidth="557.0"
              spacing="10.0"
              styleClass="up">
            <children>
                <Button fx:id="chat_room_back"
                        mnemonicParsing="false"
                        prefHeight="51.0"
                        prefWidth="52.0">
                    <graphic>
                        <ImageView fitHeight="37.0"
                                   fitWidth="30.0"
                                   pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../image/left-arrow.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets right="140.0" />
                    </HBox.margin>
                </Button>
                <Label fx:id="friend_name" />
                <ImageView fitHeight="15.0"
                           fitWidth="14.0"
                           pickOnBounds="true"
                           preserveRatio="true"
                           fx:id="online">
                </ImageView>
            </children>
        </HBox>
    </children>
<Pane>
